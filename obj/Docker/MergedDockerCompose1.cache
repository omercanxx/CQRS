services:
  cqrs.api:
    build:
      context: C:\Users\omer.sucu\Documents\GitHub\CQRS
      dockerfile: CQRS.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CQRS.API
      target: base
    container_name: CQRS.API
    depends_on:
      rabbitmq:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: cqrsapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/CQRS.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8080
      target: 80
    - target: 80
    - published: 8001
      target: 443
    - target: 443
    tty: true
    volumes:
    - C:\Users\omer.sucu\Documents\GitHub\CQRS\CQRS.API:/app:rw
    - C:\Users\omer.sucu\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\omer.sucu\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\omer.sucu\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\omer.sucu\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\omer.sucu\Documents\GitHub\CQRS:/src:rw
  rabbitmq:
    container_name: rabbitmq
    environment:
      RABBITMQ_DEFAULT_PASS: '123456'
      RABBITMQ_DEFAULT_USER: admin
    image: rabbitmq:3-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
version: '3.4'